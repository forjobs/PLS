
figure
plot((RESULTS.X_Loadings(:,1)), ...
    (RESULTS.X_Loadings(:,2)), 'o',...
    'MarkerFaceColor', 'blue')
text((RESULTS.X_Loadings(:,1)),...
    (RESULTS.X_Loadings(:,2)), ...
    X_TABLE.Properties.VariableNames)
hold on
plot((RESULTS.Y_Loadings(:,1)), ...
    (RESULTS.Y_Loadings(:,2)), 'o',...
    'MarkerFaceColor', 'blue')
text((RESULTS.Y_Loadings(:,1)),...
    (RESULTS.Y_Loadings(:,2)), ...
    Y_TABLE.Properties.VariableNames)

title('X- and Y-loadings on the first two PLS components');
xlabel('PLS-1');
ylabel('PLS-2');

figure
plot(scaledata(RESULTS.X_Scores(:,1),-1,1), ...
    scaledata(RESULTS.X_Scores(:,2),-1,1), '*')
text(scaledata(RESULTS.X_Scores(:,1),-1,1),...
    scaledata(RESULTS.X_Scores(:,2),-1,1), ...
    X_TABLE.Properties.RowNames)
hold on
plot(scaledata(RESULTS.Y_Loadings(:,1),-1,1), ...
    scaledata(RESULTS.Y_Loadings(:,2),-1,1), 'o',...
    'MarkerFaceColor', 'blue')
text(scaledata(RESULTS.Y_Loadings(:,1),-1,1),...
    scaledata(RESULTS.Y_Loadings(:,2),-1,1), ...
    Y_TABLE.Properties.VariableNames)

title('BIPLOT of the first two PLS components');
xlabel('PLS-1');
ylabel('PLS-2');
axis([-1.2,1.2,-1.2,1.2]);